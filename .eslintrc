{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins" : [
        "react"
    ],
    "rules": {
      "no-use-before-define": 0,
      "no-param-reassign": 0,
      "no-console": 0,
      "react/jsx-filename-extension":  [1, { "extensions": [".js", ".jsx"] }]
      },
      "globals": {
          "document": true
      }
}
